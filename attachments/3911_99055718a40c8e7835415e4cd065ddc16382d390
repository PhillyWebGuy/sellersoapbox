              <h1 class="title" id="page-title">Select committee on Benghazi</h1>
            


<!-- node.tpl.php -->
<article class="node-205419 node node-video view-mode-full clearfix" about="/video/house/205419-select-committee-on-benghazi" typeof="sioc:Item foaf:Document">
  <div class="clearfix">
    <div class="addthis_sharing_toolbox" data-url="http://thehill.com/video/house/205419-select-committee-on-benghazi" data-title="Select committee on Benghazi"></div>
<div class="share">
  <a href="mailto:?subject=TheHill.com%3A%20%20data-title%3D%22Select%20committee%20on%20Benghazi%22&amp;body=From%20TheHill.com%3A%20%0A%0A%0A%0Ahttp%3A//thehill.com/video/house/205419-select-committee-on-benghazi" class="email">&nbsp</a>
  </div>    <a href="http://thehill.com/video/house/205419-select-committee-on-benghazi#disqus_thread" class="disqus-comments" data-disqus-identifier="node/205419"></a>  </div>
      <header>
                  <span property="dc:title" content="Select committee on Benghazi" class="rdf-meta element-hidden"></span>
          </header>
        <p class="submitted">
      By <span rel="sioc:has_creator">A.B. Stoddard, columnist, The Hill</span> - <span class="date">05/06/14 08:58 PM EDT</span>
    </p>
    <div id="ndn-video-single" class="ndn-widget" style="display: none;"></div>
  <div class="field field-name-body field-type-text-with-summary field-label-hidden"><div class="field-items"><div class="field-item even" property="content:encoded"></div></div></div>  <div class="article-tags">
        
